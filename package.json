{
  "name": "angular-webpack-stack",
  "version": "0.1.0",
  "description": "A sample project showcasing Yarn, webpack and TypeScript with React for the Linux.com article series.",
  "main": "src/index.js",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:kenzanlabs/angular-webpack-stack.git"
  },
  "keywords": [
    "kenzanlabs"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && webpack --config ./webpack.config.prod.js -p",
    "clean": "rimraf ./build && rimraf ./reports && mkdir ./reports && mkdir ./reports/test-results",
    "develop": "webpack-dev-server --open --config ./webpack.config.develop.js",
    "lint": "eslint *.js",
    "serve": "npm run build && http-server ./build -o -p 9000",
    "test": "rimraf ./reports && jest --coverage --config ./jest.config.json",
    "tdd": "npm run test -- --watch"
  },
  "devDependencies": {
    "@types/jest": "^20.0.5",
    "@types/node": "^8.0.19",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.2",
    "babel-loader": "^7.1.1",
    "chalk": "^2.0.1",
    "eslint": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "jest": "^20.0.4",
    "jest-junit-reporter": "^1.1.0",
    "jest-preset-angular": "^2.0.5",
    "node-sass": "^4.5.3",
    "progress-bar-webpack-plugin": "^1.10.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@angular/common": "^4.3.2",
    "@angular/compiler": "^4.3.2",
    "@angular/core": "^4.3.2",
    "@angular/platform-browser": "^4.3.2",
    "@angular/platform-browser-dynamic": "^4.3.2",
    "es6-shim": "^0.35.3",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.8.16"
  }
}
